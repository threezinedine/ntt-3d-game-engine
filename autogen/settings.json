{
	"blueprints": [
		{
			"file": "core/.vscode/c_cpp_properties.json.in",
			"output": "core/.vscode/c_cpp_properties.json"
		}
	],
	"commands": [
		{
			"files": "autogen/*.txt.in",
			"command": "cat {{ INPUT }} > {{ INPUT[:-3] }}"
		}
	],
	"bindings": [
		{
			"output": "editor/Engine/__init__.pyi",
			"input": "core/include/engine/engine.h",
			"dependencies": ["core/include/**/*.h"],
			"includes": ["{{ BASE_DIR }}/core/include"],
			"template": "pyi.j2"
		}
	]
}
